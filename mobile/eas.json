{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://staging.your-domain.com/api/v1",
        "EXPO_PUBLIC_MINIO_URL": "https://staging.your-domain.com:8002"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.your-domain.com/api/v1",
        "EXPO_PUBLIC_MINIO_URL": "https://storage.your-domain.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
